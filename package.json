{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js --watch",
    "test": "node ace test",
    "lint": "eslint app/",
    "lint:fix": "eslint --fix app/",
    "migrations": "ENV_SILENT=true node ace migration:rollback --force && node ace migration:run --force",
    "db": "npm run migrations && node ace seed"
  },
  "engines": {
    "node": "8.5.0"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/vow": "^1.0.17",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "sqlite3": "^4.1.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  },
  "autoload": {
    "App": "./app"
  }
}
